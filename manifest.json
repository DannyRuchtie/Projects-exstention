{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "css": [ "styles.css" ],
      "js": [ "content.js" ],
      "matches": [ "https://github.com/*", "https://codesandbox.io/*", "https://codesandbox.test/*", "https://codesandbox.stream/*", "https://*.build.csb.dev/*" ]
   } ],
   "description": "Quickly import repositories in to CodeSandbox Projects. This extentions adds an 'Open in CodeSandbox Projects' button to GitHub repos",
   "homepage_url": "https://codesandbox.io",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "CodeSandbox",
   "permissions": [ "\u003Call_urls>" ],
   "short_name": "CodeSandbox",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.2"
}
